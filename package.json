        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-plugin-checker": "^0.6.4",
        "vite-plugin-full-reload": "^1.1.0",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.2.2",
