{
    "name": "portfolio",
    "version": "4.1.0",
    "date": "21 Jul 2023 21:52",
    "homepage": "https://elpandafriki.github.io/portfolio/",
    "private": true,
    "type": "module",
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview --port 8080",
        "fix_version": "npm run exec_update_version && prettier --loglevel silent --write -c package.json",
        "exec_update_version": "ts-node --project ./scripts/tsconfig.json ./scripts/update_version.ts",
        "exec_commit_version": "ts-node --project ./scripts/tsconfig.json ./scripts/commit_version.ts",
        "update-version": "npm run fix_version && npm run exec_commit_version",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "deploy": "gh-pages -d build",
        "format": " ts-node --project ./scripts/tsconfig.json ./scripts/format.ts",
        "test": "vitest"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.6",
        "@testing-library/jest-dom": "^6.1.3",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/node": "^20.6.0",
        "@types/react": "^18.2.23",
        "@types/react-dom": "^18.2.7",
        "@types/react-redux": "^7.1.26",
        "@types/redux-logger": "^3.0.9",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.3",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "@vitest/browser": "^0.34.4",
        "@vitest/coverage-istanbul": "^0.34.4",
        "@vitest/coverage-v8": "^0.34.4",
        "@vitest/ui": "^0.34.4",
        "eslint": "^8.50.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "gh-pages": "^6.0.0",
        "happy-dom": "^12.2.0",
        "jsdom": "^22.1.0",
        "prettier": "^3.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.11.0",
