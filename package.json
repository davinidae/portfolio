{
    "name": "portfolio",
    "version": "4.1.0",
    "date": "21 Jul 2023 21:52",
    "homepage": "https://elpandafriki.github.io/portfolio/",
    "private": true,
    "type": "module",
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview --port 8080",
        "fix_version": "npm run exec_update_version && prettier --loglevel silent --write -c package.json",
        "exec_update_version": "ts-node --project ./scripts/tsconfig.json ./scripts/update_version.ts",
        "exec_commit_version": "ts-node --project ./scripts/tsconfig.json ./scripts/commit_version.ts",
        "update-version": "npm run fix_version && npm run exec_commit_version",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "deploy": "gh-pages -d build",
        "format": " ts-node --project ./scripts/tsconfig.json ./scripts/format.ts",
        "test": "vitest"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.1.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "^20.11.16",
        "@types/react": "^18.2.54",
        "@types/react-dom": "^18.2.18",
        "@types/react-redux": "^7.1.33",
        "@types/redux-logger": "^3.0.13",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react-swc": "^3.6.0",
        "@vitest/browser": "^1.2.2",
        "@vitest/coverage-istanbul": "^1.2.2",
        "@vitest/coverage-v8": "^1.2.2",
        "@vitest/ui": "^1.2.2",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "gh-pages": "^6.1.1",
        "happy-dom": "^13.3.8",
        "jsdom": "^24.0.0",
        "prettier": "^3.2.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.0.1",
        "react-redux": "^9.1.0",
        "redux-logger": "^3.0.6",
